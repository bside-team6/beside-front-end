    "axios": "^0.23.0",
    "bootstrap": "^5.1.3",
    "date-fns": "^2.25.0",
    "emotion-reset": "^3.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.17.4",
    "react-js-pagination": "^3.0.3",
    "react-query": "^3.27.0",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^4.0.3",
    "react-select": "^5.1.0",
    "recoil": "^0.4.1",
    "styled-components": "^5.3.3"
