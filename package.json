{
  "name": "wine-fe",
  "description": "디오니소스 FE",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "analyze": "ANALYZE=TRUE craco build",
    "test": "craco test",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint": "eslint './src/**/*.{js,jsx,ts,tsx}'",
    "type-check": "tsc -p tsconfig.json",
    "storybook": "start-storybook -p 6006 -s public --modern",
    "build-storybook": "build-storybook -s public --modern",
    "deploy-storybook": "storybook-to-ghpages"
  },
  "dependencies": {
    "@coreui/coreui": "^4.0.5",
    "@coreui/react": "^4.0.2",
    "@craco/craco": "^6.4.2",
    "@emotion/react": "^11.7.0",
    "@emotion/styled": "^11.6.0",
    "@hookform/resolvers": "^2.8.3",
    "@loadable/component": "^5.15.0",
    "awesome-debounce-promise": "^2.1.0",
    "axios": "^0.24.0",
    "bootstrap": "^5.1.3",
    "date-fns": "^2.26.0",
    "emotion-reset": "^3.0.1",
    "framer-motion": "^4.1.17",
    "immer": "^9.0.7",
    "intersection-observer": "^0.12.0",
    "lodash-es": "^4.17.21",
    "rc-pagination": "^3.1.14",
    "react": "^17.0.2",
    "react-date-picker": "^8.3.6",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.4",
    "react-hook-form": "^7.20.4",
    "react-hot-toast": "^2.1.1",
    "react-js-pagination": "^3.0.3",
    "react-query": "^3.33.6",
    "react-router-dom": "^6.0.2",
    "react-scripts": "^4.0.3",
    "react-select": "^5.2.1",
    "recoil": "^0.5.2",
    "swiper": "^7.3.1",
    "typescript": "^4.5.2",
    "utility-types": "^3.10.0",
    "zod": "^3.11.6"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.3.0",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/node-logger": "^6.3.12",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.12",
    "@storybook/storybook-deployer": "^2.8.10",
    "@trivago/prettier-plugin-sort-imports": "^3.1.1",
    "@types/loadable__component": "^5.13.4",
    "@types/lodash-es": "^4.17.5",
    "@types/node": "^16.11.10",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "craco-alias": "^3.0.1",
    "http-proxy-middleware": "^2.0.1",
    "msw": "^0.35.0",
    "msw-storybook-addon": "^1.4.1",
    "prettier": "^2.5.0",
    "sass": "^1.43.5",
    "storybook-preset-craco": "0.0.6",
    "webpack-bundle-analyzer": "^4.5.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "rules": {
      "react-hooks/exhaustive-deps": [
        "warn",
        {
          "additionalHooks": "(useRecoilCallback|useRecoilTransaction_UNSTABLE)"
        }
      ]
    },
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 safari version"
    ]
  }
}
